{
  "ConnectionStrings": {
    "DbConnection":  "Server=localhost; Database=myApi; Port=5432; User Id=root; Password=root"
  },
  "ApiConfiguration": {
    "ApiName": "My Api",
    "ApiVersion": "v1",
    "ApiBaseUrl": "https://localhost:7132",
    "IdentityServerBaseUrl": "http://localhost:8080/auth/realms/demo",
    "OidcSwaggerUIClientId": "my-api",
